    "jasmine-core": "~2.6.3",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-jasmine": "~1.1.0",
    "karma-typescript": "~3.0.3",
    "rollup": "~0.58.2",
    "rollup-plugin-typescript2": "~0.13.0",
    "rollup-plugin-uglify": "~3.0.0",
